generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = env("TURSO_DATABASE_URL")
}

model Users {
  uuid      String  @id @default(uuid())
  username  String  @unique
  email     String  @unique
  salt      String  @unique
  hash      String  @unique
}
